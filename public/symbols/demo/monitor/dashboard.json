{
  "dataBindings": [
    {
      "attr": "value",
      "valueType": "String",
      "defaultValue": "0A",
      "name": "转盘文字"
    },
    {
      "attr": "angle",
      "valueType": "Number",
      "defaultValue": 135,
      "name": "转盘角度"
    },
    {
      "attr": "maxValue",
      "valueType": "Number",
      "defaultValue": 100,
      "name": "转盘最大刻度值"
    }
  ],
  "width": 200,
  "height": 200,
  "comps": [
    {
      "type": "components/monitor/dashboard.json",
      "displayName": "dashboard",
      "rect": [
        -50,
        -50,
        300,
        300
      ],
      "maxValue": {
        "func": "attr@maxValue",
        "value": "100"
      },
      "value": {
        "func": "attr@value",
        "value": "0A"
      },
      "angle": {
        "func": "attr@angle",
        "value": 135
      }
    }
  ]
}